<%= form_for @book do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :author %>
    <%= f.text_field :author %>
  </p>
  <p>
    <%= f.label :genre %>
    <%= f.text_field :genre %>
  </p>
  <p>
    <%= f.label :summary %>
    <%= f.text_area :summary %>
  </p>
  <%= f.submit 'Add New Book'%>
<% end %>
<%= link_to 'Back', books_path %>
